[fedora-modular]
name=NEXUS Fedora Modular $releasever - $basearch
baseurl=https://{{ repository }}/repository/fedora-home/releases/$releasever/Modular/$basearch/os/
enabled = 0
countme=1
metadata_expire=7d
repo_gpgcheck=0
type=rpm
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-$basearch
skip_if_unavailable=False
sslverify=0
